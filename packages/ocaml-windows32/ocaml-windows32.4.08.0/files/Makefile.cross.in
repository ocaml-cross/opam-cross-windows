TARGET=i686-w64-mingw32
PREFIX=%{prefix}%/windows-sysroot
BINDIR=$(PREFIX)/bin
LIBDIR=$(PREFIX)/lib/ocaml
STUBLIBDIR=$(LIBDIR)/stublibs
MANDIR=$(PREFIX)/man
PROGRAMS_MAN_SECTION=1
LIBRARIES_MAN_SECTION=3
BYTERUN=ocamlrun
CAMLRUN=%{bin}%/ocamlrun
OCAMLRUN=%{bin}%/ocamlrun
CAMLYACC=%{bin}%/ocamlyacc
OCAMLYACC=%{bin}%/ocamlyacc
RANLIB=%{conf-gcc-windows32:prefix}%ranlib
RANLIBCMD=%{conf-gcc-windows32:prefix}%ranlib
ARCMD=%{conf-gcc-windows32:prefix}%ar
HASHBANGSCRIPTS=false
UNIX_OR_WIN32=win32
UNIXLIB=win32unix
GRAPHLIB=win32graph
FLAMBDA=%{conf-flambda-windows:installed?true:false}%
WITH_FLAMBDA_INVARIANTS=false
WITH_SPACETIME=%{conf-spacetime-windows:installed?true:false}%
ENABLE_CALL_COUNTS=false
LIBUNWIND_AVAILABLE=false
LIBUNWIND_LINK_FLAGS=
WITH_PROFINFO=%{conf-spacetime-windows:installed?true:false}%
PROFINFO_WIDTH=0
FORCE_SAFE_STRING=false
DEFAULT_SAFE_STRING=true
WINDOWS_UNICODE=1
AFL_INSTRUMENT=false
AWK=gawk
FORCE_SAFE_STRING=false

PTHREAD_LINK=
PTHREAD_CAML_LINK=
X11_INCLUDES=
X11_LINK=
TK_DEFS=
TK_LINK=
LIBBFD_LINK=
CC=%{conf-gcc-windows32:prefix}%gcc
CPP=%{conf-gcc-windows32:prefix}%cpp
OC_CFLAGS=-O2 -mms-bitfields -fno-strict-aliasing -Wall -fno-tree-vrp
OC_CPPFLAGS=-DCAML_NAME_SPACE -DUNICODE -D_UNICODE -DWINDOWS_UNICODE=$(WINDOWS_UNICODE)
OCAMLC_CFLAGS=-O2 -mms-bitfields
OCAMLOPT_CFLAGS=-O2 -mms-bitfields
LDFLAGS=-municode
BYTECCDBGCOMPOPTS=-g
BYTECCLIBS=-lws2_32 -lversion
RPATH=
EXE=.exe
EMPTY=
OUTPUTEXE=-o $(EMPTY)
OUTPUTOBJ=-o $(EMPTY)
SUPPORTS_SHARED_LIBRARIES=true
SHAREDCCCOMPOPTS=
MKSHAREDLIBRPATH=
NATDYNLINKOPTS=
SYSLIB=-l$(1)
#ml let syslib x = "-l"^x;;

### How to build a static library
MKLIB=%{conf-gcc-windows32:prefix}%ar rc $(1) $(2); %{conf-gcc-windows32:prefix}%ranlib $(1)
#ml let mklib out files opts = Printf.sprintf "%{conf-gcc-windows32:prefix}%ar rc %s %s %s; %{conf-gcc-windows32:prefix}%ranlib %s" out opts files out;;
ARCH=amd64
MODEL=default
SYSTEM=mingw
OCAML_OS_TYPE=Win32
NATIVECCPROFOPTS=$(BYTECCCOMPOPTS)
NATIVECCLIBS=$(BYTECCLIBS)
ASM=%{conf-gcc-windows32:prefix}%as
ASPP=$(CC) -c
ASPPPROFFLAGS=
PROFILING=false
DYNLINKOPTS=
OTHERLIBRARIES=win32unix str dynlink bigarray systhreads
DEBUGGER=ocamldebugger
CC_PROFILE=-pg
SYSTHREAD_SUPPORT=true
PARTIALLD=%{conf-gcc-windows32:prefix}%ld -r
PACKLD=$(PARTIALLD) -o
IFLEXDIR=-I%{flexdll-windows:lib}%
O=o
A=a
SO=so
EXT_OBJ=.o
EXT_ASM=.s
EXT_LIB=.a
EXT_DLL=.dll
EXTRALIBS=
CCOMPTYPE=cc
TOOLCHAIN=mingw
NATDYNLINK=$(SUPPORTS_SHARED_LIBRARIES)
CMXS=cmxa
FLEXLINK=%{flexdll-windows:lib}%/flexlink -chain $(SYSTEM) -stack 16777216 -link -static-libgcc
MKEXE=$(MKEXE_ANSI) $(if $(LDFLAGS),-link "$(LDFLAGS)")
MKEXE_BOOT=$(CC) $(CFLAGS) $(LDFLAGS) $(OUTPUTEXE)$(1) $(2)
MKEXE_ANSI=$(FLEXLINK) -exe
MKEXEDEBUGFLAG=-g
MKDLL=$(FLEXLINK)
MKMAINDLL=$(FLEXLINK) -maindll
RUNTIMED=true
SHARED=shared
WITH_DEBUGGER=ocamldebugger
WITH_OCAMLDOC=ocamldoc
ASM_CFI_SUPPORTED=false
WITH_FRAME_POINTERS=false
FLAT_FLOAT_ARRAY=true
CC_HAS_DEBUG_PREFIX_MAP=false
AS_HAS_DEBUG_PREFIX_MAP=false
INSTALL_BYTECODE_PROGRAMS=true
